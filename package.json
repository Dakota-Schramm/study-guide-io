{
  "name": "study-guide-io",
  "description": "",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "biome:check": "biome ci ./src",
    "biome:fix": "biome check --apply-unsafe ./src",
    "lint-staged:comment": "/* figure out how to get lint staged to work better? */",
    "format": "pnpm dlx @biomejs/biome format ./src",
    "format:fix": "pnpm dlx @biomejs/biome format --write ./src",
    "format:help": "pnpm dlx @biomejs/biome format --help ",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --apply ./src",
    "prepare": "husky install",
    "test:run": "pnpm exec playwright test",
    "test:ui": "pnpm exec playwright test --ui",
    "test:debug": "pnpm exec playwright test --debug",
    "tdd:test": "pnpm cucumber-js -t '@browser'",
    "tdd:spec": "pnpm cucumber-js -t 'not @browser'"
  },
  "dependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@headlessui/react": "^1.7.18",
    "next": "14.1.0",
    "pdf-lib": "^1.17.1",
    "react": "^18",
    "react-dom": "^18",
    "react-pdf": "^7.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@biomejs/biome": "1.5.3",
    "@playwright/test": "^1.41.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^29.7.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
